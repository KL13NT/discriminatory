	"scripts": {
		"develop": "node --max-old-space-size=512 node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode development --port 8080 --config webpack.dev.config.js --env.api='localhost:3000'",
		"storybook": "start-storybook --port 8080 --ci",
		"watch": "run-p watch:webpack watch:server",
		"watch:webpack": "webpack --watch --mode production --config webpack.prod.config.js",
		"watch:server": "http-server ./build -p 8080",
		"prebuild": "yarn lint && yarn fix && yarn test",
		"build": "yarn build:webpack && yarn build:storybook",
		"build:webpack": "yarn webpack --mode production --config webpack.prod.config.js --env.api=ec2-15-188-50-153.eu-west-3.compute.amazonaws.com",
		"build:storybook": "build-storybook -c .storybook -o docs",
		"deploy-pages": "gh-pages -d docs",
		"lint": "eslint --ext .js,.jsx ./src --config .eslintrc.js --fix --global window",
		"fix": "prettier --write **/*.jsx **/*.js",
		"test": "jest --colors --expand --logHeapUsage --config jest.config.js --verbose --coverage --passWithNoTests && yarn cypress:run",
		"cypress:open": "cypress open --config-file ./cypress-config.json",
		"cypress:run": "cypress run --config-file ./cypress-config.json"
	},
